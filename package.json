  "license": "-",
  "bugs": {
    "url": "https://github.com/cskeppstedt/larz-client/issues"
  },
  "homepage": "https://github.com/cskeppstedt/larz-client",
  "devDependencies": {
    "autoprefixer-core": "^5.1.11",
    "css-loader": "^0.9.1",
    "file-loader": "^0.8.1",
    "jsx-loader": "^0.12.2",
    "nib": "^1.1.0",
    "phantomjs": "^1.9.19",
    "postcss-loader": "^0.4.0",
    "style-loader": "^0.8.3",
    "stylus": "^0.49.3",
    "stylus-loader": "^0.6.0",
    "webpack": "^1.5.3",
    "webpack-dev-server": "^1.7.0",
    "webpack-reload-plugin": "^0.1.2"
  },
  "dependencies": {
